{
  "name": "SYGEFEX-backend-formik",
  "description": "the backend of Systeme de Gestion des Examens (SYGEFEX  BACK-END  Formik)",
  "scripts": {
    "start": "nodemon -e js,graphql -x node src/index.js",
    "dev": "nodemon -e js,graphql -x node --inspect=50000 src/index.js",
    "test": "jest",
    "deploy": "prisma deploy --env-file variables.env",
    "reset": "prisma reset --env-file variables.env",
    "delete": "prisma delete --env-file variables.env",
    "schema": "prisma get-schema --env-file variables.env"
  },
  "dependencies": {
    "apollo-server-express": "^2.15.1",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "client-sessions": "^0.8.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "6.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.16.7",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.2",
    "graphql-yoga": "1.17.4",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^4.6.8",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.3",
    "prisma": "1.17.1",
    "prisma-binding": "2.1.6",
    "stripe": "^6.12.1",
    "util": "^0.12.3"
